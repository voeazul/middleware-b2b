{
  "openapi": "3.0.1",
  "info": {
      "title": "Sales.B2B.Authentication.Api",
      "description": "The Authentication service is responsible for authenticating the systemic user of the B2B Middleware. Obtaining the token through the Authentication service is mandatory at the beginning of each B2B Middleware session.",
      "version": "1.0"
  },
  "servers": [
      {
          "url": "https://general-api-tst.voeazul.com.br/sales/b2b/authentication"
      }
  ],
  "paths": {
      "/api/v1/token": {
          "post": {
              "tags": [
                  "Token"
              ],
              "summary": "token v1",
              "description": "<p>\r\n  <b>Description: </b>\r\n  <br />\r\n            This method creates the general access token that will grant access to the Agency API.<br /></p>\r\n<br />",
              "operationId": "apiv1token_Token_POSTBy",
              "responses": {
                  "201": {
                      "description": "Returns the general access token.",
                      "content": {
                          "text/plain": {
                              "schema": {
                                  "$ref": "#/components/schemas/Sales.B2B.Authentication.Api.Domain.Commands.v1.GenerateToken.GenerateTokenCommandResponse"
                              },
                              "examples": {
                                  "default": {
                                      "value": null
                                  }
                              }
                          },
                          "application/json": {
                              "schema": {
                                  "$ref": "#/components/schemas/Sales.B2B.Authentication.Api.Domain.Commands.v1.GenerateToken.GenerateTokenCommandResponse"
                              },
                              "example": {
                                  "token": "string"
                              }
                          },
                          "text/json": {
                              "schema": {
                                  "$ref": "#/components/schemas/Sales.B2B.Authentication.Api.Domain.Commands.v1.GenerateToken.GenerateTokenCommandResponse"
                              },
                              "example": {
                                  "token": "string"
                              }
                          }
                      }
                  },
                  "400": {
                      "description": "The request is invalid."
                  },
                  "401": {
                      "description": "The given token is invalid or has expired."
                  },
                  "404": {
                      "description": "Indicates a resource not found within the Token sent."
                  },
                  "500": {
                      "description": "An unhandled internal error has occurred."
                  }
              }
          },
          "get": {
              "tags": [
                  "Token"
              ],
              "summary": "token v1",
              "description": "<p>\r\n  <b>Description: </b>\r\n  <br />\r\n            Keeps the active token alive.<br /></p>\r\n<br />\r\n<p>\r\n  <b>Requirements: </b>\r\n  <br />\r\n            Authenticated JWT token as bearer token in request header.<br /></p>\r\n<br />",
              "operationId": "apiv1token_Token_GETByProvidertype",
              "parameters": [
                  {
                      "name": "providerType",
                      "in": "query",
                      "schema": {
                          "enum": [
                              "NewSkies"
                          ],
                          "type": "string"
                      }
                  }
              ],
              "responses": {
                  "200": {
                      "description": "Returns basic session data."
                  },
                  "400": {
                      "description": "The request is invalid."
                  },
                  "401": {
                      "description": "The given token is invalid or has expired."
                  },
                  "404": {
                      "description": "Indicates a resource not found within the Token sent."
                  },
                  "500": {
                      "description": "An unhandled internal error has occurred."
                  }
              }
          },
          "delete": {
              "tags": [
                  "Token"
              ],
              "summary": "token v1",
              "description": "<p>\r\n  <b>Description: </b>\r\n  <br />\r\n            This method abandons the active token.<br /></p>\r\n<br />\r\n<p>\r\n  <b>Requirements: </b>\r\n  <br />\r\n            Authenticated JWT token as bearer token in request header.<br /></p>\r\n<br />",
              "operationId": "apiv1token_Token_DELETEBy",
              "responses": {
                  "200": {
                      "description": ""
                  },
                  "400": {
                      "description": "The request is invalid."
                  },
                  "401": {
                      "description": "The given token is invalid or has expired."
                  },
                  "404": {
                      "description": "Resource not found."
                  },
                  "500": {
                      "description": "An internal server error has occurred."
                  }
              }
          }
      }
  },
  "components": {
      "schemas": {
          "Sales.B2B.Authentication.Api.Domain.Commands.v1.GenerateToken.GenerateTokenCommandResponse": {
              "type": "object",
              "properties": {
                  "token": {
                      "type": "string",
                      "description": "Generated access token that grants access to the Agency API.",
                      "nullable": true
                  }
              },
              "additionalProperties": false
          },
          "Sales.B2B.Authentication.Api.Infrastructure.Service.Shared.v1.ProviderType": {
              "enum": [
                  "NewSkies"
              ],
              "type": "string"
          }
      },
      "securitySchemes": {
          "apiKeyHeader": {
              "type": "apiKey",
              "name": "Ocp-Apim-Subscription-Key",
              "in": "header"
          },
          "apiKeyQuery": {
              "type": "apiKey",
              "name": "subscription-key",
              "in": "query"
          }
      }
  },
  "security": [
      {
          "apiKeyHeader": []
      },
      {
          "apiKeyQuery": []
      }
  ]
}